package db2redis;

public class ForumUser {

	private String login;
	
	private String city;
	
	private long date;

	public ForumUser(String login, String city, long date) {
		super();
		this.login = login;
		this.city = city;
		this.date = date;
	}

	public String getLogin() {
		return login;
	}

	public void setLogin(String login) {
		this.login = login;
	}

	public String getCity() {
		return city;
	}

	public void setCity(String city) {
		this.city = city;
	}

	public long getDate() {
		return date;
	}

	public void setDate(long date) {
		this.date = date;
	}
	
	
}
